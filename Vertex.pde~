import java.util.*;

public class Vertex {
    int x;
    int y;
    List<Vertex> neighbors;

    public Vertex(int x, int y) {
	this.x = x;
	this.y = y;
	neighbors = new LinkedList<>();
    }

    public void add(Vertex v) {
	v.neighbors.add(this);
	neighbors.add(v);
    }

    public void print() {
	Set<Vertex> visited = new HashSet<>();
	Stack<Vertex> eval = new Stack<>();
	eval.push(this);
	while (!eval.empty()) {
	    Vertex v = eval.pop();
	    visited.add(v);
	    for (Vertex n: v.neighbors) {
		if (!visited.contains(n)) {
		    line(x, y, n.x, n.y);
		    eval.push(n);
		}
	    }
	}
    }	
}